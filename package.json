{
    "name": "rinku",
    "productName": "rinku",
    "version": "1.0.0-prototype.20",
    "description": "Rinku (link)",
    "main": "./dist/index.js",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "npm run build && electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint .",
        "build": "tsc",
        "watch": "tsc --watch",
        "test": "tsc -p ./tests && mocha ./tests/out/tests/index.js",
        "prettier": "prettier --write .",
        "postinstall": "npm run build && npm run prettier",
        "prestart": "npm run build"
    },
    "homepage": "./",
    "keywords": [],
    "author": {
        "name": "RepleDEV",
        "email": "54616266+RepleDEV@users.noreply.github.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {
                "ignore": [
                    "tests/",
                    "src/",
                    "temp/"
                ]
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "rinku"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    },
    "dependencies": {
        "@repledev/rinku_winswitcher": "^1.0.14",
        "electron-squirrel-startup": "^1.0.0",
        "jquery": "^3.5.1",
        "lodash": "^4.17.20",
        "robotjs": "^0.6.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "6.0.0-beta.53",
        "@electron-forge/maker-deb": "6.0.0-beta.53",
        "@electron-forge/maker-rpm": "6.0.0-beta.53",
        "@electron-forge/maker-squirrel": "6.0.0-beta.53",
        "@electron-forge/maker-zip": "6.0.0-beta.53",
        "@types/lodash": "^4.14.161",
        "@types/mocha": "^8.0.3",
        "@types/node": "^12.12.62",
        "@typescript-eslint/eslint-plugin": "^4.3.0",
        "@typescript-eslint/parser": "^4.3.0",
        "electron": "^10.1.2",
        "eslint": "^7.10.0",
        "foreman": "^3.0.1",
        "mocha": "^8.1.3",
        "prettier": "2.1.2",
        "react-scripts": "^3.4.3",
        "typescript": "^4.0.2"
    }
}
