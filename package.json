{
    "name": "rinku",
    "version": "1.0.0-prototype-40",
    "description": "Rinku (link)",
    "main": "./dist/main.js",
    "scripts": {
        "dev": "npm-run-all -p dev:react electron:serve",
        "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && npm run start:electron",
        "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack/react.webpack.js --mode development",
        "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
        "start:electron": "electron .",
        "build": "npm-run-all build:electron build:react",
        "build:run": "npm-run-all build start:electron",
        "build:electron": "webpack --config webpack/electron.webpack.js",
        "build:react": "webpack --config webpack/react.webpack.js",
        "package": "npm-run-all build package:dist",
        "package:dist": "electron-builder --dir",
        "lint": "eslint .",
        "prettier": "prettier --write .",
        "postinstall": "npm run rebuild",
        "rebuild": "electron-rebuild"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-es2015": "^7.0.0-beta.53",
        "@babel/preset-react": "^7.12.1",
        "@babel/preset-typescript": "^7.12.1",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/jquery": "^3.5.3",
        "@types/lodash": "^4.14.162",
        "@types/react": "^16.9.53",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.0",
        "electron": "^10.1.4",
        "electron-devtools-installer": "^3.1.1",
        "electron-rebuild": "^2.2.0",
        "eslint": "^7.11.0",
        "eslint-plugin-react": "^7.21.5",
        "html-webpack-plugin": "^4.5.0",
        "husky": "^4.3.0",
        "node-loader": "^1.0.2",
        "npm-run-all": "^4.1.5",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "style-loader": "^2.0.0",
        "typescript": "^4.0.3",
        "typescript-plugin-css-modules": "^2.7.0",
        "wait-on": "^5.2.0",
        "webpack": "^5.1.3",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    },
    "dependencies": {
        "@repledev/rinku_winswitcher": "^1.0.15",
        "electron-squirrel-startup": "^1.0.0",
        "jquery": "^3.5.1",
        "lodash": "^4.17.20",
        "robotjs": "^0.6.0"
    }
}
